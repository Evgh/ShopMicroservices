{
  "Database": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "shopApiDb",
    "ShopsCollectionName": "shops",
    "LocationsCollectionName": "locations"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "My Health Checks",
        "Uri": "/health"
      }
    ],
    "EvaluationTimeInSeconds": 5
  },
  "AllowedHosts": "*"
}
